
{
  "appId": "com.lovable.flypcast",
  "appName": "FlypCast",
  "webDir": "dist",
  "server": {
    "url": "http://localhost:8080",
    "cleartext": true
  },
  "ios": {
    "contentInset": "always",
    "backgroundColor": "#ffffff",
    "scheme": "app"
  },
  "android": {
    "allowMixedContent": true
  },
  "plugins": {
    "SplashScreen": {
      "launchAutoHide": false,
      "showSpinner": false
    }
  }
}
